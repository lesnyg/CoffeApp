<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edu05.coffeapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.coffeeActivity"></activity>
        <activity android:name=".activities.OrderCheckActivity" />
        <activity android:name=".activities.ForResultActivity"></activity>
        <activity android:name=".activities.TargetActivity" />
        <activity android:name=".activities.WebBrowserActivity"></activity>
        <activity android:name=".activities.WeatherActivity"></activity>
        <activity android:name=".activities.Memo2Activity" />
        <activity
            android:name=".activities.MemoActivity"
            android:label="@string/title_activity_memo"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".activities.BankActivity"></activity>
        <activity android:name=".activities.OpenAccountActivity" />
        <activity android:name=".activities.AccountCheckActivity" />
        <activity android:name=".activities.AdminModeActivity" />
        <activity android:name=".ListviewActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LifeCycleActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>